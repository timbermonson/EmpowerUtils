{
    "type": "module",
    "name": "empowernode",
    "version": "1.0.0",
    "main": "app.js",
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "scripts": {
        "addr": "tsx watch --include ./ioFiles/input.txt apps/ScrapeAddresses/run.ts",
        "addrAll": "tsx watch --include ./ioFiles/input.txt apps/ScrapeAddresses/run.ts -o both",
        "addrAllMult": "tsx watch --include ./ioFiles/input.txt apps/ScrapeAddresses/run.ts -o both -c -m",
        "ebp": "tsx watch --include ./ioFiles/input.txt apps/ExtractBusinessPrincipals/run.ts",
        "ebpMult": "tsx watch --include ./ioFiles/input.txt apps/ExtractBusinessPrincipals/run.ts -m",
        "end": "tsx watch --include ./ioFiles/input.txt apps/EndatoApp/app.ts --no-stdin",
        "xero": "tsx apps/Xero/app.ts",
        "xeroserve": "tsx apps/Xero/oauthServer.ts",
        "tsx": "tsx",
        "integration": "vitest run --config ./.test/config/integration.config.js",
        "test-unit": "vitest --config ./.test/config/unit.config.js",
        "test-unitCI": "vitest run --config ./.test/config/unit.config.js",
        "coverage": "vitest run --config ./.test/config/unit.config.js --coverage"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@datastructures-js/priority-queue": "^6.3.3",
        "@inquirer/prompts": "^7.4.1",
        "@types/command-line-args": "^5.2.3",
        "@types/config": "^3.3.5",
        "@types/express": "^5.0.2",
        "@types/inquirer": "^9.0.7",
        "@types/jquery": "^3.5.32",
        "@types/jsdom": "^21.1.7",
        "@types/lodash-es": "^4.17.12",
        "@types/ws": "^8.18.1",
        "ajv": "^8.17.1",
        "axios": "^1.8.1",
        "binary-search": "^1.3.6",
        "chalk": "^5.4.1",
        "clipboardy": "^4.0.0",
        "command-line-args": "^6.0.1",
        "config": "^3.3.12",
        "dayjs": "^1.11.13",
        "express": "^5.1.0",
        "fastpriorityqueue": "^0.7.5",
        "fuse.js": "^7.1.0",
        "heap-js": "^2.6.0",
        "inquirer": "^12.5.2",
        "jquery": "^3.7.1",
        "jsdom": "^26.0.0",
        "lodash-es": "^4.17.21",
        "qs": "^6.14.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3",
        "ws": "^8.18.1",
        "xero-node": "^11.1.0"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^3.0.9",
        "vitest": "^3.0.9"
    }
